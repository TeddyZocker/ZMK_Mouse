   &none		&none			&kp TILDE		&kp LS(GRAVE)		&kp LESS_THAN		&kp GREATER_THAN		&kp UNDER		&kp EXCL		&kp LS(SLASH)		&none			&kp RA(E)		&kp RA(MINUS) 
									&caps_word		&none			&trans				&trans			&none			&to 8
                        >;
                };
